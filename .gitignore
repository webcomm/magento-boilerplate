# Start: Cron files
/cron.php
/cron.sh

# Start: Misc files
/downloader
/errors
/favicon.ico
/get.php
/includes

# Start: Index and installer files
/index.php.sample
/install.php

# Start: Global javascript
/js/*

# Start: PHP libraries
/lib/*
